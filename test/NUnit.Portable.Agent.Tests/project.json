{
  "version": "3.4.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "warningsAsErrors": true,
    "define": [
    ],
    "compile": {
      "include": "./**/*.cs",
      "includeFiles": [
        "../../src/CommonAssemblyInfo.cs"
      ]
    },
    "copyToOutput": "MockTestResult.xml"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "netstandard1.3",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        },
        "System.IO.FileSystem": "4.0.1",
        "System.Console": "4.0.0"
      }
    },
    "net451": {}
  },

  "dependencies": {
    "NUnit.Portable.Agent": {
      "target": "project"
    },
    "mock-assembly": {
      "target": "project"
    },
    "NUnitLite": "3.5.0"  }
}